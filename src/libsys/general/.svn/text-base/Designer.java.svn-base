package libsys.general;

import libsys.general.LIBSYSSystem;

public class Designer
{
		LIBSYSSystem LibrarySystem;
		
		public FrameDesigner frameDesigner;
		public PanelDesigner panelDesigner;
		
		public Designer(LIBSYSSystem LibrarySystem)
		{
				this.LibrarySystem = LibrarySystem;
				
				frameDesigner = new FrameDesigner(LibrarySystem);
				panelDesigner = new PanelDesigner(LibrarySystem);
		}
}