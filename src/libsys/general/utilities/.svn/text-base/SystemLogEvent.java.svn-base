package libsys.general.utilities;

import libsys.general.LIBSYSSystem;

public class SystemLogEvent
{		
		public Integer EventID;
		public String EventName;
		public String EventTime;
		public String Username;
		
		public SystemLogEvent()
		{
				this.EventID = new Integer(-1);
				this.EventName = null;
				this.EventTime = null;
				this.Username = null;
		}
		
		public SystemLogEvent(int EventID, String EventName, String EventTime, String Username)
		{
				this.EventID = new Integer(EventID);
				this.EventName = EventName;
				this.EventTime = EventTime;
				this.Username = Username;
		}		
}
